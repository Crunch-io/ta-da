<script>
    $(document).ready(function(){
    $('.code-block .sqs-block-content .h2-wrapper').addClass('ui-closed').css('cursor','pointer');
    $(".code-block .sqs-block-content .h2-wrapper").nextUntil(".h2-wrapper").slideToggle();
    $(".code-block .sqs-block-content .h2-wrapper").click(function() {
      
      $(this).nextUntil(".h2-wrapper").slideToggle();
      $(this).toggleClass('ui-closed ui-open');
      });
    });
    </script>

 <script>
 var bannerheight = 10; // number of pixels to scroll before applying classes
 
 $(window).bind('scroll', function () {
     if ($(window).scrollTop() > bannerheight) {
         $('#navigation-bottom').addClass('sticky-nav');
         $('#content-wrapper').addClass('sticky-nav-gap');
     } else {
         $('#navigation-bottom').removeClass('sticky-nav');
         $('#content-wrapper').removeClass('sticky-nav-gap');
     }
 });  
 </script>


